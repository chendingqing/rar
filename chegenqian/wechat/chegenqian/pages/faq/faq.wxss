/* pages/faq/faq.wxss */
.question{
	display: flex;
	flex-direction: column;
}
.question navigator{
	height: 88rpx;
	border-top: 2rpx solid #f5f5f5;
	background-color: #fff;
	display: flex;
}
.que_lable,.que_ic{
	flex: 1;
	display: block;
	font-size: 30rpx;
}
.que_lable{
	padding-left: 20rpx;
	line-height: 88rpx;
	color:#333;
}
.que_ic image{
	width: 28rpx;
	height: 48rpx;
	margin-top: 21rpx;
	float: right;
	margin-right: 20rpx;
}